{
	"_version": "1.12.0",

	"sap.app": {
		"id": "pub.pubsample",
		"type": "application",
		"title": "pubsample",
		"description": "pubsample",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"ach": "set-ach",
		"resources": "resources.json",
		"dataSources": {
			"mainService": {
				"uri": "srv/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},

	"sap.fiori": {
		"registrationIds": [
		],
		"archeType": "transactional"
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://task",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "pub.pubsample.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},

		"dependencies": {
			"minUI5Version": "1.66.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {}
			}
		},

		"contentDensities": {
			"compact": true,
			"cozy": true
		},

		"models": {
			"": {
				"dataSource": "mainService",
				"preload": true
			}
		},

		"resources": {
			"css": [
				{ "uri": "css/webfonts/pretendard-subset.css" },
				{ "uri": "css/pub.css" }
			]
		  },

		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "pub.pubsample.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"bypassed": {
					"target": ["notFound"]
				},
				"async": true
			},

			"routes": [
				{
					"pattern": "",
					"name": "worklist",
					"target": ["worklist"]
				},
				{
					"pattern": "template1",
					"name": "template1",
					"target": ["template1"]
				},
				{
					"pattern": "Products/{objectId}",
					"name": "object",
					"target": ["object"]
				}
			],

			"targets": {
				"worklist": {
					"viewName": "Worklist",
					"viewId": "worklist",
					"viewLevel": 1,
					"title": "Components"
				},
				"template1": {
					"viewName": "Template1",
					"viewId": "template1",
					"viewLevel": 1,
					"title": "Template 1"
				},
				"object": {
					"viewName": "Object",
					"viewId": "object",
					"viewLevel": 2,
					"title": "Components"
				},
				"objectNotFound": {
					"viewName": "ObjectNotFound",
					"viewId": "objectNotFound"
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				}
			}
		}
	}
}
<mvc:View
	controllerName="pub.pubsample.controller.Worklist"
	xmlns="sap.m"
	xmlns:table="sap.ui.table"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:pub="sap.ui.pub.control"
>
	<Page
		navButtonTooltip=""
		title="Components"
	>
		<content>
			<VBox width="auto">
				<HBox
					class="sapUiResponsiveMargin"
					width="auto"
				>
					<VBox>
						<Title
							level="H3"
							text="Navigation"
						/>
						<HBox>
							<Breadcrumbs
								currentLocationText="신규신청"
								separatorStyle="GreaterThan"
							>
								<Link text="복리후생"/>
								<Link text="경조금 신청"/>
							</Breadcrumbs>
						</HBox>
					</VBox>
				</HBox>
				<HBox
					class="sapUiResponsiveMargin"
					width="auto"
				>
					<VBox width="100%">
						<Title
							level="H3"
							text="Tab container"
						/>
						<HBox>
							<IconTabBar selectedKey="Attachments">
								<layoutData>
									<FlexItemData growFactor="1"/>
								</layoutData>
								<items>
									<IconTabFilter
										text="Info"
										key="Info"
									>
										<Text text="Info content goes here ..."/>
									</IconTabFilter>
									<IconTabFilter
										text="Attachments"
										key="Attachments"
									>
										<Text text="Attachments go here ..."/>
									</IconTabFilter>
									<IconTabFilter
										text="Notes"
										key="Notes"
									>
										<Text text="Notes go here ..."/>
									</IconTabFilter>
									<IconTabFilter
										text="People"
										key="People"
									>
										<Text text="People content goes here ..."/>
									</IconTabFilter>
								</items>
							</IconTabBar>
						</HBox>
					</VBox>
				</HBox>
				<HBox
					class="sapUiResponsiveMargin"
					width="auto"
				>
					<VBox width="100%">
						<Title
							level="H3"
							text="Panel"
						/>
						<HBox>
							<Panel
								expandable="true"
								width="auto"
							>
								<layoutData>
									<FlexItemData growFactor="1"/>
								</layoutData>
								<headerToolbar>
									<OverflowToolbar
										height="3rem"
										style="Clear"
									>
										<Title text="Custom Toolbar with a header text"/>
										<ToolbarSpacer/>
										<Button icon="sap-icon://drop-down-list"/>
									</OverflowToolbar>
								</headerToolbar>
								<content>
									<Text text="Lorem ipsum dolor st amet, consetetur sadipscing elitr, sed diam nonumy eirmod tempor invidunt ut labore et dolore magna aliquyam erat, sed diam voluptua. At vero eos et accusam et justo duo dolores et ea rebum. Stet clita kasd gubergren, no sea takimata sanctus est Lorem ipsum dolor sit amet. Lorem ipsum dolor sit amet, consetetur sadipscing elitr, sed diam nonumy eirmod tempor invidunt ut labore et dolore magna aliquyam erat, sed diam voluptua. Lorem ipsum dolor sit amet, consetetur sadipscing elitr, sed diam nonumy eirmod tempor invidunt ut labore et dolore magna aliquyam erat"/>
								</content>
							</Panel>
						</HBox>
					</VBox>
				</HBox>
				<HBox
					class="sapUiResponsiveMargin"
					width="auto"
				>
					<VBox>
						<Title
							level="H3"
							text="Dialog"
						/>
						<HBox>
							<Button
								text="사원검색"
								press=".openEmployeeDialog"
							/>
						</HBox>
					</VBox>
				</HBox>
				<HBox
					class="sapUiResponsiveMargin"
					width="auto"
				>
					<VBox>
						<Title
							level="H3"
							text="Label"
						/>
						<HBox>
							<Label
								text="Label A (required)"
								labelFor="input-a"
							/>
							<Input
								id="input-a"
								required="true"
							/>
							<Label
								text="Label B (bold)"
								labelFor="input-b"
								design="Bold"
							/>
							<Input id="input-b"/>
							<Label
								text="Label C (normal)"
								labelFor="input-c"
							/>
							<Input id="input-c"/>
						</HBox>
					</VBox>
				</HBox>
				<HBox
					class="sapUiResponsiveMargin"
					width="auto"
				>
					<VBox>
						<Title
							level="H3"
							text="Input"
						/>
						<HBox>
							<Input
								id="nameInput"
								placeholder="이름을 입력하세요."
								valueStateText="Name must not be empty. Maximum 10 characters."
								value="{
									path: '/name',
									type: 'sap.ui.model.type.String',
									constraints: {
										minLength: 1,
										maxLength: 10
									}
								}"
							/>
							<Input
								id="productInput"
								placeholder="Enter product"
								showSuggestion="true"
								showValueHelp="true"
								valueHelpRequest=".onValueHelpRequest"
								suggestionItems="{/ProductCollection}"
								class="sapIcon_Search"
							>
								<core:Item text="{Name}"/>
							</Input>
							<Input
								value="10"
								description="명"
								width="100px"
								fieldWidth="60%"
							/>
							<Input
								id="InputNoEdit"
								value="No edit"
								type="Text"
								placeholder="Product"
								enabled="true"
								editable="false"
							/>
							<Input
								id="InputDisabled"
								value="Disabled"
								type="Text"
								placeholder="Product"
								enabled="false"
							/>
							<Input
								valueState="Success"
								value="Value state Success"
							/>
							<Input
								valueState="Warning"
								valueStateText="Warning message. Extra long text used as a warning message. Extra long text used as a warning message - 2. Extra long text used as a warning message - 3. Extra long text used as a warning message - 4. Extra long text used as a warning message - 5."
								value="Value state Warning."
							/>
							<Input
								valueState="Error"
								value="Value state Error"
							/>
							<Input
								valueState="Information"
								value="Value state Information"
							/>
						</HBox>
					</VBox>
				</HBox>
				<HBox
					class="sapUiResponsiveMargin"
					width="auto"
				>
					<VBox>
						<Title
							level="H3"
							text="Search Input"
						/>
						<HBox>
							<SearchField
								width="100%"
								placeholder="검색어를 입력하세요."
							/>
						</HBox>
					</VBox>
				</HBox>
				<HBox
					class="sapUiResponsiveMargin"
					width="auto"
				>
					<VBox>
						<Title
							level="H3"
							text="TextArea"
						/>
						<HBox>
							<TextArea
								value="Lorem ipsum dolor st amet, consetetur sadipscing elitr, sed diam nonumy eirmod tempor invidunt ut labore et dolore magna aliquyam erat, sed diam voluptua. At vero eos et accusam et justo duo dolores et ea rebum. Stet clita kasd gubergren, no sea takimata sanctus est Lorem ipsum dolor sit amet. Lorem ipsum dolor sit amet, consetetur sadipscing elitr, sed diam nonumy eirmod tempor invidunt ut labore et dolore magna aliquyam erat, sed diam voluptua. Lorem ipsum dolor sit amet, consetetur sadipscing elitr, sed diam nonumy eirmod tempor invidunt ut labore et dolore magna aliquyam erat"
								rows="5"
								width="500px"
							/>
						</HBox>
					</VBox>
				</HBox>
				<HBox
					class="sapUiResponsiveMargin"
					width="auto"
				>
					<VBox>
						<Title
							level="H3"
							text="ComboBox and Select"
						/>
						<HBox>
							<ComboBox items="{
								path: '/ProductCollection',
								sorter: {
									path: 'text'
								},
								templateShareable: true
							}">
								<core:Item
									key="{ProductId}"
									text="{Name}"
								/>
							</ComboBox>
							<Select
								forceSelection="false"
								selectedKey="{/SelectedProduct}"
								items="{
									path: '/ProductCollection',
									sorter: {
										path: 'Name'
									},
									templateShareable: true
								}"
							>
								<core:Item
									key="{ProductId}"
									text="{Name}"
								/>
							</Select>
							<MultiComboBox
								selectionChange="handleSelectionChange"
								selectionFinish="handleSelectionFinish"
								width="350px"
								items="{
									path: '/ProductCollection',
									sorter: {
										path: 'Name'
									},
									templateShareable: true
								}"
							>
								<core:Item
									key="{ProductId}"
									text="{Name}"
								/>
							</MultiComboBox>
						</HBox>
					</VBox>
				</HBox>
				<HBox
					class="sapUiResponsiveMargin"
					width="auto"
				>
					<VBox>
						<Title
							level="H3"
							text="Radio Button"
						/>
						<HBox>
							<RadioButtonGroup
								id="rbg1"
								width="100%"
								columns="6"
								class="sapUiMediumMarginBottom"
							>
								<pub:RadioButton
									id="RB1-1"
									text="Long Option Number 1"
									outerSize="50%"
									innerSize="35%"
								/>
								<pub:RadioButton
									id="RB1-2"
									text="Option 2"
									enabled="false"
									outerSize="50%"
									innerSize="35%"
								/>
								<pub:RadioButton
									id="RB1-3"
									text="Nr. 3"
									editable="false"
									outerSize="50%"
									innerSize="35%"
								/>
								<pub:RadioButton
									id="RB1-4"
									text="Long Option 4"
									outerSize="50%"
									innerSize="35%"
								/>
								<pub:RadioButton
									id="RB1-5"
									text="Option 5"
									outerSize="50%"
									innerSize="35%"
								/>
								<pub:RadioButton
									id="RB1-6"
									text="Nr. 6"
									outerSize="50%"
									innerSize="35%"
								/>
							</RadioButtonGroup>
						</HBox>
					</VBox>
				</HBox>
				<HBox
					class="sapUiResponsiveMargin"
					width="auto"
				>
					<VBox>
						<Title
							level="H3"
							text="CheckBox"
						/>
						<HBox>
							<CheckBox
								text="Option a"
								selected="true"
							/>
							<CheckBox text="Option b"/>
							<CheckBox
								text="Option c"
								selected="true"
							/>
							<CheckBox text="Option d"/>
							<CheckBox
								text="Option e"
								enabled="false"
							/>
							<CheckBox
								text="Option partially selected"
								selected="true"
								partiallySelected="true"
							/>
							<CheckBox
								text="Warning"
								valueState="Warning"
							/>
							<CheckBox
								text="Warning disabled"
								valueState="Warning"
								enabled="false"
								selected="true"
							/>
							<CheckBox
								text="Error"
								valueState="Error"
							/>
							<CheckBox
								text="Error disabled"
								valueState="Error"
								enabled="false"
								selected="true"
							/>
							<CheckBox
								text="Information"
								valueState="Information"
							/>
							<CheckBox
								text="Information disabled"
								valueState="Information"
								enabled="false"
								selected="true"
							/>
							<CheckBox
								text="Checkbox with wrapping='true' and long text"
								wrapping="true"
								width="150px"
							/>
						</HBox>
					</VBox>
				</HBox>
				<HBox
					class="sapUiResponsiveMargin"
					width="auto"
				>
					<VBox>
						<Title
							level="H3"
							text="Date and Time Picker "
						/>
						<HBox>
							<DatePicker
								id="DP1"
								placeholder="Enter Date"
								class="sapUiSmallMarginBottom sapIcon_Date"
							/>
							<DateRangeSelection
								id="DRS2"
								class="sapIcon_Date"
							/>
							<TimePicker
								id="TP1"
								width="100%"
								placeholder="Enter Time"
								valueFormat="HH:mm"
								displayFormat="HH:mm"
								minutesStep="5"
								class="sapIcon_Time"
							/>
						</HBox>
					</VBox>
				</HBox>
				<HBox
					class="sapUiResponsiveMargin"
					width="auto"
				>
					<VBox>
						<Title
							level="H3"
							text="Switch"
						/>
						<HBox>
							<Switch state="true">
								<layoutData>
									<FlexItemData growFactor="1"/>
								</layoutData>
							</Switch>
							<Switch
								state="true"
								enabled="false"
							>
								<layoutData>
									<FlexItemData growFactor="1"/>
								</layoutData>
							</Switch>
							<Switch
								state="true"
								customTextOn=" "
								customTextOff=" "
							>
								<layoutData>
									<FlexItemData growFactor="1"/>
								</layoutData>
							</Switch>
							<Switch
								state="true"
								customTextOn=" "
								customTextOff=" "
								enabled="false"
							>
								<layoutData>
									<FlexItemData growFactor="1"/>
								</layoutData>
							</Switch>
							<Switch
								type="AcceptReject"
								state="true"
							>
								<layoutData>
									<FlexItemData growFactor="1"/>
								</layoutData>
							</Switch>
							<Switch
								type="AcceptReject"
								state="true"
								enabled="false"
							>
								<layoutData>
									<FlexItemData growFactor="1"/>
								</layoutData>
							</Switch>
						</HBox>
					</VBox>
				</HBox>
				<HBox
					class="sapUiResponsiveMargin"
					width="auto"
				>
					<VBox>
						<Title
							level="H3"
							text="Button"
						/>
						<HBox>
							<Button
								text="Default"
								ariaDescribedBy="defaultButtonDescription genericButtonDescription"
							>
								<layoutData>
									<FlexItemData growFactor="1"/>
								</layoutData>
							</Button>
							<Button
								type="Accept"
								text="Accept"
								ariaDescribedBy="acceptButtonDescription genericButtonDescription"
							>
								<layoutData>
									<FlexItemData growFactor="1"/>
								</layoutData>
							</Button>
							<Button
								type="Reject"
								text="Reject"
								ariaDescribedBy="rejectButtonDescription genericButtonDescription"
							>
								<layoutData>
									<FlexItemData growFactor="1"/>
								</layoutData>
							</Button>
							<Button
								text="Coming Soon"
								ariaDescribedBy="comingSoonButtonDescription genericButtonDescription"
								enabled="false"
							>
								<layoutData>
									<FlexItemData growFactor="1"/>
								</layoutData>
							</Button>
							<Button
								type="Emphasized"
								text="Emphasized"
							/>
							<Button text="Default"/>
							<Button
								text="Transparent"
								type="Transparent"
							/>
							<Button
								icon="sap-icon://action"
								type="Transparent"
								text="With Icon"
							/>
						</HBox>
					</VBox>
				</HBox>
				<HBox
					class="sapUiResponsiveMargin"
					width="auto"
				>
					<VBox>
						<Title
							level="H3"
							text="Segmented Button"
						/>
						<HBox>
							<SegmentedButton
								width="200px"
								selectedKey="10"
							>
								<items>
									<SegmentedButtonItem
										text="전체"
										key="all"
									/>
									<SegmentedButtonItem
										text="재직자"
										key="10"
									/>
									<SegmentedButtonItem
										text="퇴직자"
										key="20"
									/>
								</items>
							</SegmentedButton>
						</HBox>
					</VBox>
				</HBox>
				<HBox
					class="sapUiResponsiveMargin"
					width="auto"
				>
					<VBox>
						<Title
							level="H3"
							text="Message Box"
						/>
						<HBox>
							<Button
								text="Confirm"
								press=".onConfirmationMessageBoxPress"
							/>
							<Button
								text="Alert"
								press=".onAlertMessageBoxPress"
							/>
							<Button
								text="Error"
								press=".onErrorMessageBoxPress"
							/>
							<Button
								text="Info"
								press=".onInfoMessageBoxPress"
							/>
							<Button
								text="Warning"
								press=".onWarningMessageBoxPress"
							/>
							<Button
								text="Success"
								press=".onSuccessMessageBoxPress"
							/>
						</HBox>
					</VBox>
				</HBox>
				<HBox
					class="sapUiResponsiveMargin"
					width="auto"
				>
					<VBox>
						<Title
							level="H3"
							text="Message Strip"
						/>
						<HBox>
							<MessageStrip
								text="Default (Information) with default icon and close button:"
								showIcon="true"
								showCloseButton="true"
								class="sapUiMediumMarginBottom"
							/>
							<MessageStrip
								text="Error with default icon and close button:"
								type="Error"
								showIcon="true"
								showCloseButton="true"
								class="sapUiMediumMarginBottom"
							/>
							<MessageStrip
								text="Warning with default icon and close button:"
								type="Warning"
								showIcon="true"
								showCloseButton="true"
								class="sapUiMediumMarginBottom"
							/>
						</HBox>
						<HBox>
							<MessageStrip
								text="Success with default icon and close button:"
								type="Success"
								showIcon="true"
								showCloseButton="true"
								class="sapUiMediumMarginBottom"
							/>
							<MessageStrip
								text="Information with default icon."
								type="Information"
								showIcon="true"
								class="sapUiMediumMarginBottom"
							/>
						</HBox>
						<HBox>
							<MessageStrip
								text="Information with custom icon"
								type="Information"
								showIcon="true"
								customIcon="sap-icon://locked"
								class="sapUiMediumMarginBottom"
							/>
							<MessageStrip
								text="Error with link"
								type="Error"
								showCloseButton="true"
								class="sapUiMediumMarginBottom"
							>
								<link>
									<Link
										text="Open SAP Homepage"
										target="_blank"
										href="http://www.sap.com"
									/>
								</link>
							</MessageStrip>
						</HBox>
					</VBox>
				</HBox>
				<HBox
					class="sapUiResponsiveMargin"
					width="auto"
				>
					<VBox>
						<Title
							level="H3"
							text="Progress Indicator"
						/>
						<HBox>
							<ProgressIndicator
								class="sapUiSmallMarginBottom"
								percentValue="30"
								displayValue="30%"
								showValue="true"
								state="None"
								width="200px"
							/>
							<ProgressIndicator
								class="sapUiSmallMarginBottom"
								percentValue="50"
								showValue="false"
								state="Error"
								width="200px"
							/>
						</HBox>
						<HBox>
							<ProgressIndicator
								class="sapUiSmallMarginBottom"
								percentValue="99"
								displayValue="0.99GB of 1GB"
								showValue="true"
								state="Success"
								width="200px"
							/>
							<ProgressIndicator
								class="sapUiSmallMarginBottom"
								percentValue="25"
								displayValue="25%"
								showValue="true"
								state="Warning"
								width="200px"
							/>
							<ProgressIndicator
								class="sapUiSmallMarginBottom"
								percentValue="40"
								displayValue="40%"
								showValue="true"
								state="Information"
								width="200px"
							/>
						</HBox>
					</VBox>
				</HBox>
				<HBox
					class="sapUiResponsiveMargin"
					width="auto"
				>
					<VBox>
						<Title
							level="H3"
							text="Table"
						/>
						<table:Table
							width="90%"
							rows="{/ProductCollection}"
							selectionMode="MultiToggle"
							visibleRowCount="7"
							ariaLabelledBy="title"
						>
							<table:extension>
								<OverflowToolbar style="Clear">
									<Title
										id="title"
										text="Products"
									/>
								</OverflowToolbar>
							</table:extension>
							<table:columns>
								<table:Column
									width="11rem"
									hAlign="Center"
									filterProperty="name"
									sortProperty="name"
								>
									<Label text="Product Name"/>
									<table:template>
										<Text
											text="{Name}"
											wrapping="false"
										/>
									</table:template>
								</table:Column>
								<table:Column
									width="11rem"
									hAlign="Center"
									filterProperty="ProductId"
									sortProperty="ProductId"
								>
									<Label text="Product Id"/>
									<table:template>
										<Input value="{ProductId}"/>
									</table:template>
								</table:Column>
								<table:Column
									width="6rem"
									hAlign="Center"
									filterProperty="Quantity"
									sortProperty="Quantity"
								>
									<Label text="Quantity"/>
									<table:template>
										<Text text="{Quantity}"/>
									</table:template>
								</table:Column>
								<table:Column
									width="9rem"
									hAlign="Center"
									filterProperty="Status"
									sortProperty="Status"
